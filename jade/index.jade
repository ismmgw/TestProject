        include ./blocks/header.jade
        .icon_layout
            .container
                .row.icon
                    .col-md-3
                        .row
                            .col-xs-4
                                i.fa.fa-users.icon_font(aria-hidden="true")
                            .col-xs-8.icon_text
                                    span.icon_text_top 6 823
                                    br
                                    p.icon_text_bot инвесторов
                    .col-md-3
                        .row
                            .col-xs-4
                                i.fa.fa-rub.icon_font(aria-hidden="true")
                            .col-xs-8.icon_text
                                span.icon_text_top 7 782 640 P
                                br
                                span.icon_text_bot инвестировано
                    .col-md-3
                        .row
                            .col-xs-4
                                i.fa.fa-stack-overflow.icon_font(aria-hidden="true")
                            .col-xs-8.icon_text
                                span.icon_text_top 738 175 P
                                br
                                span.icon_text_bot выплачено
                    .col-md-3
                        .row
                            .col-xs-4
                                i.fa.fa-trophy.icon_font(aria-hidden="true")
                            .col-xs-8.icon_text
                                span.icon_text_top 100%
                                br
                                span.icon_text_bot выплат
        .tariff_head
