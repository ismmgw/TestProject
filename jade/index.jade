        .applet_layout
            .container
                .row
                    .col-md-4
                        .applet
                            .applet_percent 8%
                            .row
                                .col-xs-6
                                    .row
                                        .col-xs-3.text-right.padding_zero
                                            img(src="./img/clock.png")
                                        .col-xs-9.text-left
                                            p 10 дней
                                .col-xs-6.applet_days_right
                                    span 500 - 4999 Р
                                    br
                                    span.applet_days_right_bot диапазон размера вкладов
                            .applet_slider
                                .row
                                    .col-xs-4.text-left
                                        input.applet_slider_input(type="text" id="amount" readonly)
                                    .col-xs-8.text-right
                                        div.applet_slider(id="slider")
